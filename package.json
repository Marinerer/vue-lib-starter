{
  "name": "vue-starter",
  "version": "1.0.0",
  "scripts": {
    "reinstall": "rimraf node_modules && pnpm install",
    "dev": "pnpm vue3",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "clean": "rimraf dist",
    "vue2": "pnpm use:vue2 && vite --config ./playground/vue2/vite.config.ts ./playground/vue2",
    "vue3": "pnpm use:vue3 && vite --config ./playground/vue3/vite.config.ts ./playground/vue3",
    "use:vue2": "vue-demi-switch 2.7 vue",
    "use:vue3": "vue-demi-switch 3 vue3"
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vitejs/plugin-vue2-jsx": "^1.0.2",
    "@vue/tsconfig": "^0.4.0",
    "npm-run-all2": "^6.1.1",
    "rimraf": "^5.0.5",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vue": "^2.7.14",
    "vue-tsc": "^1.8.19",
    "vue3": "npm:vue@3.3.7"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.7.2",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}
